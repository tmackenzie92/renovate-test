{
  "dependencyDashboard": true,
  "automerge": false,
  "enabledManagers": [
    "sbt"
  ],
  "packageRules": [
    {
      "matchPackagePrefixes": [
        "com.fasterxml.jackson"
      ],
      "groupName": "Jackson"
    },
    {
      "matchPackagePrefixes": [
        "io.cucumber"
      ],
      "groupName": "Cucumber"
    }
  ],
  "separateMinorPatch": true,
  "minor": {
    "ignoreDeps": [
      "com.typesafe.akka:akka-protobuf",
      "com.typesafe.akka:akka-persistence",
      "com.typesafe.akka:akka-persistence-query"
    ]
  },
  "major": {
    "ignoreDeps": [
      "com.typesafe.akka:akka-protobuf",
      "com.typesafe.akka:akka-persistence",
      "com.typesafe.akka:akka-persistence-query"
    ]
  }
}