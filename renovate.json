{
  "dependencyDashboard": true,
  "automerge": false,
  "enabledManagers": [
    "sbt"
  ],
  "packageRules": [
    {
      "matchPackagePrefixes": [
        "com.fasterxml.jackson"
      ],
      "groupName": "Jackson"
    },
    {
      "matchPackagePrefixes": [
        "io.cucumber"
      ],
      "groupName": "Cucumber"
    }
  ],
  "separateMinorPatch": true,
  "minor": {
    "packageRules": [
      {
        "matchPackagePrefixes": [
          "com.typesafe.akka"
        ],
        "enabled": false
      }
    ]
  },
  "major": {
    "ignoreDeps": [
      "io.cucumber:cucumber-junit",
      "io.cucumber:cucumber-scala"
    ]
  }
}